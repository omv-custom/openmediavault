{
  "name": "openmediavault-workbench",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "upgrade": "npm run upgrade:minor",
    "upgrade:minor": "ncu --upgrade --target minor",
    "upgrade:patch": "ncu --upgrade --target patch",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "build:prod": "ng build --configuration production --verbose",
    "test": "ng test --watch --no-cache",
    "test:ci": "ng test --no-cache --max-workers=4",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint:tsc": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.spec.json --noEmit",
    "lint:eslint": "ng lint",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "run-p -cl --aggregate-output lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:scss": "stylelint \"**/*.scss\" --fix",
    "fix": "run-s -cl fix:*",
    "i18n": "npm run i18n:extract",
    "i18n:extract": "transloco-keys-manager extract --sort --replace",
    "e2e": "start-test http://localhost:4201 'cypress open'",
    "e2e:ci": "start-test http://localhost:4201 'cypress run -b chrome --headless'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/cdk": "^19.2.1",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.1",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/pwa": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/service-worker": "^19.2.0",
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/commands": "^6.8.0",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-python": "^6.1.7",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.10.8",
    "@codemirror/legacy-modes": "^6.4.3",
    "@codemirror/lint": "^6.8.4",
    "@codemirror/search": "^6.5.10",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.36.4",
    "@fontsource/inter": "^5.2.5",
    "@fontsource/roboto-mono": "^5.2.5",
    "@jsverse/transloco": "^7.5.1",
    "@jsverse/transloco-keys-manager": "^6.1.0",
    "@mdi/angular-material": "^7.2.96",
    "@siemens/ngx-datatable": "22.4.1",
    "@typescript-eslint/types": "^8.26.0",
    "@typescript-eslint/utils": "^8.26.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "angular-feather": "^6.5.1",
    "angular-tabler-icons": "^3.26.0",
    "chart.js": "^4.4.8",
    "chartjs-plugin-datalabels": "^2.2.0",
    "core-js": "~3.41.0",
    "cronstrue": "^2.56.0",
    "dayjs": "^1.11.13",
    "glob": "^11.0.1",
    "lodash": "^4.17.21",
    "ngx-scrollbar": "^18.0.0",
    "ngx-toastr": "^19.0.0",
    "nunjucks": "^3.2.4",
    "rxjs": "~7.8.2",
    "socket.io-client": "^4.8.1",
    "strip-ansi": "^7.1.0",
    "tslib": "^2.8.1",
    "validator": "^13.12.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.0",
    "@angular-eslint/builder": "^19.2.0",
    "@angular-eslint/eslint-plugin": "^19.2.0",
    "@angular-eslint/eslint-plugin-template": "^19.2.0",
    "@angular-eslint/schematics": "^19.2.0",
    "@angular-eslint/template-parser": "^19.2.0",
    "@angular/cli": "^19.1.8",
    "@angular/compiler-cli": "^19.2.0",
    "@angular/language-service": "^19.2.0",
    "@types/chart.js": "^2.9.41",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.9",
    "@types/nunjucks": "^3.2.6",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "cypress": "~14.1.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "html-linter": "^1.1.1",
    "htmllint-cli": "^0.0.7",
    "npm-check-updates": "^17.1.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.10",
    "stylelint": "^16.15.0",
    "stylelint-config-standard-scss": "~14.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
